{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": [
		"standard-with-typescript",
		"plugin:react/recommended",
		"standard-with-typescript",
    "plugin:react/recommended",
    "plugin:react/recommended",
    "airbnb",
    "airbnb/hooks",
    "react",
    "react-native",
    "prettier",
    "prettier/react"
	],
	"compilerOptions": {
		"strictNullChecks": true
	},
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": [
		"react"
	],
	"rules": {
		"no-unused-vars": "error"
	}
}
